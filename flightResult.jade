        script.
            $('.camNavbar__toggle').click(function () {
                $('.camNavbar__main').addClass('open');
            });

            $('.camNavbar__close, .camNavbar__backdrop').click(function () {
                $('.camNavbar__main').removeClass('open');
            });
            if (screen.width < 768) {
                $('.camNavbar__link').click(function () {
                    $(this).next('.camNavbar__sub').slideToggle(200);
                    $(this).parent('li').toggleClass('active');
                });
            };
            if (screen.width < 768) {
                $('.footer__top').css('background-image', 'url("images/footer-bg-mobile.jpg")')
            };
        script.
            $('.t-datepicker').tDatePicker({
                autoClose: true,
                titleCheckIn: 'Depart',
                titleCheckOut: 'Return',
                titleDateRange: 'day',
                titleDateRanges: 'days',
                iconDate: '<i class="fas fa-calendar-alt"></i>',
                iconArrowTop: false
            });
        script.
            $('.btn-yourBooking').click(function () {
                $('.yourBooking').toggleClass('open');
            });
            $('.yourBooking__close').click(function () {
                $('.yourBooking').removeClass('open');
            });
        script.
            if (screen.width < 768) {
                $('.viewDetail').click(function () {
                    $(this).parents('.flTableMobile__info').next('.flTableMobile__detail').slideDown(400);
                    $(this).parents('.flTableMobile__info').slideUp(400);
                });
                $('.lessDetail').click(function () {
                    $(this).parents('.flTableMobile__detail').slideUp(400);
                    $(this).parents('.flTableMobile__detail').prev('.flTableMobile__info').slideDown(400);
                });
            };
            if (screen.width >= 768) {
                $('.viewDetail').click(function () {
                    $(this).parents('.flTable__info').next('.flTable__detail').slideDown(400);
                    $(this).parents('.flTable__info').slideUp(400);
                });
                $('.lessDetail').click(function () {
                    $(this).parents('.flTable__detail').slideUp(400);
                    $(this).parents('.flTable__detail').prev('.flTable__info').slideDown(400);
                });
            };
        script.
            if (screen.width < 768) {
                $('.flTableMobile__price .btnPrice').click(function () {
                    $(this).next('.flTableMobile__ticket').slideToggle(400);
                    $(this).toggleClass('rotate');
                });

                $('.flTableMobile__fare').click(function () {
                    $(this).parents('.flTableMobile__ticket').next('.fareCondition').addClass('open');
                });
                $('.fareCondition__close').click(function () {
                    $('.fareCondition').removeClass('open');
                });
            };
        script.
            if (screen.width >= 768) {
                $('.flTable__ticket').click(function () {
                    var fareCondition = $(this).parents('.flTable__group').find('.flTable__fare');
                    if($(this).hasClass('active')){
                        $(this).removeClass('active');
                        fareCondition.slideUp(400);
                    } else {
                        $('.flTable__ticket').removeClass('active');
                        $(this).addClass('active');
                        fareCondition.slideDown(400);

                    }
                });
            };
        script.
            $(".flDate__slide").slick({
                slidesToShow: 7,
                slidesToScroll: 1,
                //autoplay: true,
                speed: 500,
                autoplaySpeed: 5000,
                infinite: false,
                arrows: true,
                dots: false,
            });
        script.
            $('.flDateMobile__btn').click(function () {
                $(this).next('.flDateMobile__main').slideToggle('200');
                $(this).toggleClass('rotate');
            });



